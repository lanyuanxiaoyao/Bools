{
  "pluginName": "Bools",
  "author": "lanyuanxiaoyao",
  "homepage": "http://lanyuanxiaoyao.com",
  "description": "浏览器工具, Broswer tools",
  "version": "0.0.1",
  "logo": "logo.png",
  "main": "index.html",
  "preload": "preload.js",
  "platform": ["darwin"],
  "pluginSetting": {
    "single": true,
    "height": 1
  },
  "features": [{
    "code": "chrome_url",
    "explain": "Chrome 网址转二维码",
    "cmds": [{
      "type": "window",
      "label": "转换当前网址为二维码",
      "match": {
        "app": "/.*chrome.*/i"
      }
    }]
  },{
    "code": "chrome_remove_img",
    "explain": "移除页面上所有的 img 标签",
    "cmds": [{
      "type": "window",
      "label": "移除页面图片",
      "match": {
        "app": "/.*chrome.*/i"
      }
    }]
  }, {
    "code": "chrome_open_chrome://extensions/",
    "explain": "打开 Chrome 插件管理页面",
    "cmds": [{
      "type": "window",
      "label": "插件管理",
      "match": {
        "app": "/.*chrome.*/i"
      }
    }]
  },{
    "code": "chrome_open_https://chrome.google.com/webstore/category/extensions",
    "explain": "打开 Chrome 插件商店",
    "cmds": [{
      "type": "window",
      "label": "插件商店",
      "match": {
        "app": "/.*chrome.*/i"
      }
    }]
  }, {
    "code": "chrome_open_chrome://flags/",
    "explain": "打开 Chrome 插功能特性页面",
    "cmds": [{
      "type": "window",
      "label": "功能特性",
      "match": {
        "app": "/.*chrome.*/i"
      }
    }]
  }, {
    "code": "chrome_open_chrome://downloads/",
    "explain": "打开 Chrome 下载管理页面",
    "cmds": [{
      "type": "window",
      "label": "下载管理",
      "match": {
        "app": "/.*chrome.*/i"
      }
    }]
  }, {
    "code": "chrome_open_chrome://settings/clearBrowserData",
    "explain": "清除 Chrome 痕迹",
    "cmds": [{
      "type": "window",
      "label": "清除痕迹",
      "match": {
        "app": "/.*chrome.*/i"
      }
    }]
  }, {
    "code": "chrome_open_chrome://restart/",
